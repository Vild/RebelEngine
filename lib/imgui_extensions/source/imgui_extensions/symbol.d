void initSymbolFont(string name, float sizePixels, ubyte[] fontData) {
	import std.format : format;
	import std.algorithm : min;

	_fontData = fontData;
	ImGuiIO* io = igGetIO();

	ImFontConfig iconsConfig;
	iconsConfig.MergeMode = true;
	iconsConfig.PixelSnapH = true;

	string genName = format("%s, %.0fpx", name, sizePixels);
	iconsConfig.Name[0 .. min(genName.length, iconsConfig.Name.length)] = genName[0 .. min(genName.length, iconsConfig.Name.length)];

	_symbolFont = ImFontAtlas_AddFontFromMemoryTTF(io.Fonts, cast(void*)&fontData[0], cast(int)fontData.length,
			sizePixels, &iconsConfig, &charRange[0]);
}

void igSymbolButton(ImGuiSymbol symbol, const ImVec2 size = ImVec2(0, 0)) {
	import std.conv : to;
	import std.string : toStringz;

	assert(_symbolFont, "Symbol font is null!");
	string text;
	text ~= symbol;

	igButton(text.toStringz, size);

	if (igIsItemHovered()) {
		igBeginTooltip();
		igSetWindowFontScale(4);
		igText(text.toStringz);
		igEndTooltip();
	}
}
